{
  "name": "@modelsjs/request",
  "version": "0.0.1",
  "dependencies": {
    "@doc-tools/docs": "^1.27.0"
  },
  "scripts": {
    "lint": "yfm -i . -o $LINT_ROOT --lang en",
    "build": "npm run build:yfm && npm run build:mv",
    "build:yfm": "yfm -i . -o $BUILD_ROOT/$LANG --output-format md --add-map-file --lang en --lint-disabled",
    "build:mv": "mv $BUILD_ROOT/$LANG/files.json $BUILD_ROOT/files.json && mv $BUILD_ROOT/$LANG/.yfm $BUILD_ROOT/.yfm"
  }
}
